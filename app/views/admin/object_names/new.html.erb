<% content_for(:title, 'Object Nameの追加') %>
<h1>Object Nameの追加</h1>
<%= form_with model: [:admin, @object_name], local: true do |f| %>
    <%= f.label :name, class: 'form-label' %>
    <%= f.text_field :name, class: 'form-control' %>
    <%= f.submit '追加', class: 'btn btn-primary mt-3' %>
<% end %>
