<div class="text-center">
  <%= link_to new_admin_label_name_path do %>
    <button type="button" class="btn">LabelNameを追加する</button>
  <% end %>
</div>
<h1 class="mt-3">Label Name一覧</h1>
<table class="table mt-3 text-white">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">name</th>
    </tr>
  </thead>
  <tbody>
    <%= render @label_names %>
  </tbody>
</table>